    "index":{
        "class":"BNVerticalStack",
        "background":"window"
    },
    "title":{
        "class":"BNStaticTitleModule",
        "mode!=tablet":{
            "padding":"{20,16,4,16}"
        },
        "mode==tablet":{
            "padding":"{48,32,4,16}"
        }
    },
    "primaryItem":{
        "limit":1,
        "secondary":"$secondary",
        "mode==compact":{
            "$":"normalGroup"
        },
        "mode!=compact":{
            "cellClass":"BNCellItemTopStory",
            "H":1,
            "long_timestamp":true
        },
        "mode==tablet":{
            "mode==landscape":{
                "orientation":"landscape"
            },
            "summaries":true
        }
    },
    "normalGroup":{
        "secondary":"$secondary",
        "cellClass":"BNCellItemTopStory",
        "maxTopics":1,
        "mode==tablet":{
            "padding":"{0,8,0,8}",
            "cellMargins":"{16,8,0,8}",
            "summaries":true
        },
        "mode!=tablet":{
            "padding":"{0,4,0,4}",
            "cellMargins":"{8,4,0,4}"
        }
    },
    "tightGroup":{
        "$":"normalGroup",
        "mode==compact":{
            "padding":"{0,8,0,8}",
            "cellMargins":"{2,0,0,0}"
        }
    },
    "featureGroup":{
        "secondary":"$secondary",
        "summaries":true,
        "mode==compact":{
            "$":"tightGroup",
            "hideTopics":true,
            "cellClass":"BNCellItemTopStory"
        },
        "mode!=compact":{
            "cellClass":"BNCellItemFeature",
            "$":"normalGroup",
            "imagePosition":"top"
        }
    },
    "featureGroup_2col":{
        "$":"featureGroup",
        "mode!=compact":{
            "cellsPerRow":2,
            "H":2
        }
    },
    "featureItem":{
        "$":"featureGroup",
        "limit":1,
        "imageWidth":0.6666
    },
    "featureItemLarge":{
        "$":"featureGroup",
        "cellClass":"BNCellItemFeatureLarge",
        "limit":1,
        "cellMargins":"{8,0,8,0}",
        "weight":2
    },
    "featureItemSmall":{
        "$":"featureGroup",
        "limit":1,
        "cellMargins":"{8,0,8,0}",
        "weight":1
    },
    "digestTitle":{
        "$":"title",
        "mode==tablet":{
            "padding":"{40,16,4,16}"
        }
    },
    "digestGroup":{
        "secondary":"$secondary",
        "limit":2,
        "cellClass":"BNCellItemDigest",
        "mode==tablet":{
            "padding":"{0,8,0,8}",
            "cellMargins":"{4,0,0,0}"
        },
        "mode!=tablet":{
            "padding":"{0,8,0,8}",
            "cellMargins":"{2,0,0,0}"
        },
        "textPadding":"{8,8,0,0}"
    },
    "twoColumns":{
        "class":"BNHorizontalLinearLayout",
        "padding":"{0,8,0,8}",
        "modules":[
                   "$1",
                   "$2"
                   ]
    },
    "twoDigestGroups":{
        "mode!=tablet":{
            "class":"BNVerticalLinearLayout",
            "modules":[
                       "$1",
                       "$2"
                       ]
        },
        "mode==tablet":{
            "$":"twoColumns",
            "modules":[
                       {
                       "class":"BNVerticalLinearLayout",
                       "modules":"$1"
                       },
                       {
                       "class":"BNVerticalLinearLayout",
                       "modules":"$2"
                       }
                       ]
        }
    },
    "twoStackedVerticallyInPortraitAndHorizontallyInLandscape":{
        "mode==landscape":{
            "$":"twoColumns",
            "modules":[
                       {
                       "class":"BNVerticalLinearLayout",
                       "modules":"$1"
                       },
                       {
                       "class":"BNVerticalLinearLayout",
                       "modules":"$2"
                       }
                       ]
        },
        "mode==portrait":{
            "class":"BNVerticalLinearLayout",
            "modules":[
                       "$1",
                       "$2"
                       ]
        }
    },
