    "avCarousel":{
        "class":"BNHorizontalLinearLayout",
        "mode==tablet":{
            "padding":"{16,0,0,0}"
        },
        "mode!=tablet":{
            "padding":"{8,0,0,0}"
        },
        "$secondary":"$st_av_carousel",
        "modules":[
                   {
                   "class":"BNHorizontalStack",
                   "background":"#000",
                   "modules":[
                              {
                              "secondary":"$secondary",
                              "orderBy":"avcarousel",
                              "cellClass":"BNCellItemAV",
                              "orientation":"portrait",
                              "numLines":3,
                              "mode==tablet":{
                              "cellMargins":"{32,0,16,2}"
                              },
                              "mode!=tablet":{
                              "cellMargins":"{8,0,8,1}"
                              },
                              "format":[
                                        "bbc.mobile.news.format.video",
                                        "bbc.mobile.news.format.audio"
                                        ]
                              }
                              ]
                   }
                   ]
    },
    "photosGroup":{
        "$secondary":"$st_in_pictures",
        "limit":2,
        "mode==compact":{
            "$":"tightGroup",
            "summaries":true,
            "hideTopics":true
        },
        "mode!=compact":{
            "$":"normalGroup",
            "cellClass":"BNCellItemPhotoGallery",
            "maxImages":2
        },
        "mode==tablet":{
            "maxImages":3
        }
    },
    "mostPopularGroup":{
        "cellClass":"BNCellItemMostPopular",
        "limit":10,
        "primary":[
                   "bbc.mobile.news.item"
                   ],
        "mode!=tablet":{
            "padding":"{0,0,0,0}",
            "cellMargins":"{8,8,0,8}",
            "mode==compact":{
                "orientation":"landscape",
                "H":2
            },
            "mode!=compact":{
                "orientation":"portrait",
                "H":1
            }
        },
        "mode==tablet":{
            "cellMargins":"{8,8,8,8}",
            "H":3,
            "mode==portrait":{
                "orientation":"portrait"
            },
            "mode==landscape":{
                "orientation":"landscape"
            }
        }
    },
    "mostReadGroup":{
        "$":"mostPopularGroup",
        "secondary":[
                     "bbc.mobile.news.most_popular.most_read"
                     ],
        "format":[
                  "bbc.mobile.news.format.textual"
                  ],
        "inverseColorScheme":"NO"
    },
    "mostWatchedGroup":{
        "$":"mostPopularGroup",
        "secondary":[
                     "bbc.mobile.news.most_popular.most_watched"
