                       "textPosition":"left"
                       },
                       "$2":{
                       "$":"featureItemSmall",
                       "offset":1
                       }
                       },
                       {
                       "$":"twoColumns",
                       "$1":{
                       "$":"featureItemSmall",
                       "offset":2
                       },
                       "$2":{
                       "$":"featureItemLarge",
                       "offset":3,
                       "textPosition":"right"
                       }
                       },
                       {
                       "$":"twoColumns",
                       "$1":{
                       "$":"featureItemLarge",
                       "offset":4,
